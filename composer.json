{
    "name": "anjalicct/category",
    "description": "This will use to listing, add, update, view category module.",
    "type": "library",
    "license": "MIT",
    "autoload": {
        "classmap": [
            "src/database/migrations",
            "src/controllers",
            "src/models"
        ],
        "psr-4": {
            "App\\": "app/",
            "Anjalicct\\Category\\": "packages/anjalicct/category/src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Anjalicct\\Category\\CategoryServiceProvider"
            ]
        }
    },
    "authors": [
        {
            "name": "anjali",
            "email": "anjali.p@communicationcrafts.com"
        }
    ],
    "minimum-stability": "dev",
    "require": {},
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-update-cmd": [
            "@php artisan vendor:publish --tag=laravel-assets --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    }
}
